        <!--Artistas-->
        <record id="artista1" model="musictracker.artista">
            <field name="name">Enter Shikari</field>
            <field name="ubicacion">St. Albans, Hertfordshire, Reino Unido</field>
            <field name="fecha_formacion">2003-01-01</field>
            <field name="fecha_separacion">2020-06-14</field> <!--este grupo no se ha separado, es por probar que funciona bien-->
            <field name="foto" type="base64" file="musictracker/static/dummy_images/enter_shikari.png"/>
            <field name="generos" eval="[(6,0,[ref('genero1'),ref('genero2'),ref('genero3'),ref('genero4')])]"/>
        </record>
        <record id="artista2" model="musictracker.artista">
            <field name="name">glass beach</field>
            <field name="ubicacion">Seattle, WA, E.E.U.U.</field>
