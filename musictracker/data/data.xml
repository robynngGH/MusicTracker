        <record id="usuario1" model="musictracker.usuario">
            <field name="name">music_enjoyer1</field>
            <field name="nombre">Kevin</field>
            <field name="apellidos">López</field>
            <field name="fecha_nacimiento">2000-06-15</field>
            <field name="correo">kevinlopez@gmail.com</field>
            <field name="ubicacion">Adelaide, Australia</field>
            <field name="biografia">I LUV MUSIC!!!</field>
            <field name="foto" type="base64" file="musictracker/static/dummy_images/kevin.jpg"/>
        </record>
        <record id="usuario2" model="musictracker.usuario">
